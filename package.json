{
  "name": "@bijikyu/qtests",
  "version": "2.5.0",
  "description": "Comprehensive Node.js testing framework with method stubbing, console mocking, environment management, and automatic stub resolution for axios, winston, and other modules",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "type": "module",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bijikyu/qtests.git"
  },
  "bugs": {
    "url": "https://github.com/Bijikyu/qtests/issues"
  },
  "homepage": "https://github.com/Bijikyu/qtests#readme",
  "exports": {
    ".": "./dist/index.js",
    "./setup": "./dist/setup.js",
    "./setup.js": "./dist/setup.js",
    "./loader": "./loader.mjs",
    "./loader.mjs": "./loader.mjs",
    "./lib/*": "./dist/lib/*.js",
    "./lib/routeTestUtils": "./dist/lib/routeTestUtils.js",
    "./lib/errorHandling": "./dist/lib/errorHandling/index.js",
    "./lib/memory": "./dist/lib/memory/index.js",
    "./utils/*": "./dist/utils/*.js",
    "./config/*": "./dist/config/*.js"
  },
  "bin": {
    "qtests-ts-runner": "./bin/qtests-ts-runner",
    "qtests-ts-generate": "./bin/qtests-generate.mjs",
    "qtests-generate": "./bin/qtests-generate.mjs"
  },
  "scripts": {
    "prepare": "node scripts/ensure-runner.mjs",
    "postinstall": "node scripts/postinstall-scaffold.mjs",
    "pretest": "node scripts/clean-dist.mjs && node scripts/ensure-runner.mjs",
    "test": "node qtests-runner.mjs",
    "ci:verify": "node scripts/ci-verify-runner.mjs",
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "test:scalability": "node scripts/benchmarks/memory-benchmark.js",
    "test:scalability:memory": "node scripts/benchmarks/memory-benchmark.js",
    "test:scalability:database": "node scripts/benchmarks/database-benchmark.js",
    "test:scalability:pool": "node scripts/benchmarks/connection-pool-benchmark.js",
    "test:scalability:circuit": "node scripts/benchmarks/circuit-breaker-benchmark.js",
    "test:scalability:report": "node scripts/benchmarks/generate-report.js",
    "monitor:scalability": "node scripts/simple-scalability-test-clean.js"
  },
  "keywords": [
    "testing",
    "stub",
    "mock",
    "winston",
    "axios",
    "utils",
    "test-framework",
    "console-mock",
    "environment-management",
    "jest-compatible",
    "offline-testing",
    "side-effect-free"
  ],
  "author": "Q",
  "dependencies": {
    "axios": "^1.9.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.0",
    "fs-extra": "^11.3.3",
    "generic-pool": "^3.9.0",
    "happy-dom": "^20.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jest": "^29.7.0",
    "joi": "^18.0.2",
    "msw": "^2.12.7",
    "node-cache": "^5.1.2",
    "opossum": "^9.0.0",
    "p-queue": "^8.1.1",
    "p-retry": "^7.1.1",
    "p-timeout": "^7.0.1",
    "pg": "^8.17.1",
    "qerrors": "^1.2.7",
    "rate-limiter-flexible": "^5.0.5",
    "redis": "^4.6.0",
    "secure-json-parse": "^4.1.0",
    "serve": "^14.2.5",
    "sinon": "^21.0.0",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@bijikyu/csup": "^1.0.2",
    "@openai/codex": "^0.53.0",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.2",
    "@types/opossum": "^8.1.9",
    "@types/secure-json-parse": "^1.0.5",
    "@types/sinon": "^21.0.0",
    "agentsqripts": "^1.1.0",
    "babel-jest": "^30.1.2",
    "commoncontext": "^1.0.3",
    "express-rate-limit": "^7.0.0",
    "fileflows": "^1.0.5",
    "loqatevars": "^1.0.6",
    "madge": "^8.0.0",
    "npmcontext": "^1.0.4",
    "opencode-ai": "^1.1.19",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3",
    "unqommented": "^1.1.0"
  },
  "files": [
    "bin",
    "dist",
    "lib/templates",
    "templates",
    "scripts",
    "qtests-runner.mjs",
    "qtests-runner.mjs.template",
    "README.md",
    "LICENSE"
  ]
}
