{
  "name": "qtests",
  "version": "1.2.0",
  "description": "Comprehensive Node.js testing framework with method stubbing, console mocking, environment management, and automatic stub resolution for axios, winston, and other modules",
  "type": "module",
  "main": "index.ts",
  "bin": {
    "qtests-runner": "./bin/qtests-runner",
    "qtests-generate": "npx tsx bin/qtests-generate"
  },
  "scripts": {
    "test": "node qtests-runner.js"
  },
  "keywords": [
    "testing",
    "stub",
    "mock",
    "winston",
    "axios",
    "utils",
    "test-framework",
    "console-mock",
    "environment-management",
    "jest-compatible",
    "offline-testing",
    "side-effect-free"
  ],
  "author": "Q",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^24.0.0",
    "axios": "^1.9.0",
    "tsx": "^4.20.5"
  },
  "devDependencies": {
    "agentsqripts": "^1.0.8",
    "arqitect": "^1.0.7",
    "commoncontext": "^1.0.1",
    "fileflows": "^1.0.1",
    "jest": "^30.0.5",
    "loqatevars": "^1.0.6",
    "madge": "^8.0.0",
    "npmcontext": "^1.0.1",
    "quantumagent": "^1.0.5",
    "repomix": "^1.2.0",
    "ts-jest": "^29.4.1",
    "winston": "^3.17.0"
  }
}