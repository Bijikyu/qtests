{
  "name": "qtests",
  "version": "2.0.0",
  "description": "Comprehensive Node.js testing framework with method stubbing, console mocking, environment management, and automatic stub resolution for axios, winston, and other modules",
  "type": "module",
  "main": "index.ts",
  "bin": {
    "qtests-ts-runner": "./bin/qtests-ts-runner",
    "qtests-ts-generate": "./bin/qtests-generate.mjs",
    "qtests-generate": "./bin/qtests-generate.mjs"
  },
  "scripts": {
    "prepare": "node scripts/ensure-runner.mjs",
    "postinstall": "node scripts/postinstall-scaffold.mjs",
    "pretest": "node scripts/clean-dist.mjs && node scripts/ensure-runner.mjs",
    "test": "node qtests-runner.mjs",
    "ci:verify": "node scripts/ci-verify-runner.mjs",
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "testing",
    "stub",
    "mock",
    "winston",
    "axios",
    "utils",
    "test-framework",
    "console-mock",
    "environment-management",
    "jest-compatible",
    "offline-testing",
    "side-effect-free"
  ],
  "author": "Q",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.0",
    "axios": "^1.9.0",
    "dotenv": "^17.2.3",
    "opossum": "^9.0.0",
    "qerrors": "^1.2.7",
    "qgenutils": "^1.0.3",
    "redis": "^4.6.0",
    "sinon": "^21.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@bijikyu/csup": "^1.0.2",
    "@openai/codex": "^0.53.0",
    "@types/opossum": "^8.1.9",
    "@types/sinon": "^21.0.0",
    "agentsqripts": "^1.1.0",
    "arqitect": "^1.0.7",
    "babel-jest": "^30.1.2",
    "commoncontext": "^1.0.3",
    "express-rate-limit": "^7.0.0",
    "fileflows": "^1.0.3",
    "jest": "^29.7.0",
    "loqatevars": "^1.0.6",
    "madge": "^8.0.0",
    "msw": "^2.12.4",
    "npmcontext": "^1.0.4",
    "opencode-ai": "1.0.171",
    "qtests": "^2.0.0",
    "quantumagent": "^1.0.5",
    "repomix": "^1.2.0",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3",
    "unqommented": "^1.1.0",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "files": [
    "bin",
    "dist",
    "lib/templates",
    "templates",
    "scripts",
    "qtests-runner.mjs",
    "qtests-runner.mjs.template",
    "README.md",
    "LICENSE"
  ]
}
