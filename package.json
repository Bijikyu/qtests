{
  "name": "qtests",
  "version": "2.0.0",
  "description": "Comprehensive Node.js testing framework with method stubbing, console mocking, environment management, and automatic stub resolution for axios, winston, and other modules",
  "type": "module",
  "main": "index.ts",
  "bin": {
    "qtests-ts-runner": "./bin/qtests-ts-runner",
    "qtests-ts-generate": "./bin/qtests-generate.mjs",
    "qtests-generate": "./bin/qtests-generate.mjs"
  },
  "scripts": {
    "prepare": "node scripts/ensure-runner.mjs",
    "postinstall": "node scripts/postinstall-scaffold.mjs",
    "pretest": "node scripts/clean-dist.mjs && node scripts/ensure-runner.mjs",
    "test": "node qtests-runner.mjs",
    "ci:verify": "node scripts/ci-verify-runner.mjs",
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "test:scalability": "node scripts/benchmarks/memory-benchmark.js",
    "test:scalability:memory": "node scripts/benchmarks/memory-benchmark.js",
    "test:scalability:database": "node scripts/benchmarks/database-benchmark.js",
    "test:scalability:pool": "node scripts/benchmarks/connection-pool-benchmark.js",
    "test:scalability:circuit": "node scripts/benchmarks/circuit-breaker-benchmark.js",
    "test:scalability:report": "node scripts/benchmarks/generate-report.js",
    "monitor:scalability": "node scripts/monitoring/scalability-monitor.js"
  },
  "keywords": [
    "testing",
    "stub",
    "mock",
    "winston",
    "axios",
    "utils",
    "test-framework",
    "console-mock",
    "environment-management",
    "jest-compatible",
    "offline-testing",
    "side-effect-free"
  ],
  "author": "Q",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.2",
    "@types/node": "^24.0.0",
    "@types/secure-json-parse": "^1.0.5",
    "axios": "^1.9.0",
    "bash-language-server": "^5.6.0",
    "chai": "^5.3.3",
    "clinic": "^13.0.0",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.3",
    "generic-pool": "^3.9.0",
    "happy-dom": "^17.2.3",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "^18.0.2",
    "langchain": "^0.3.37",
    "node-cache": "^5.1.2",
    "opossum": "^9.0.0",
    "p-queue": "^8.1.1",
    "p-retry": "^7.1.1",
    "p-timeout": "^7.0.1",
    "qerrors": "^1.2.7",
    "qgenutils": "^1.0.3",
    "rate-limiter-flexible": "^5.0.5",
    "redis": "^4.6.0",
    "secure-json-parse": "^4.1.0",
    "sinon": "^21.0.0",
    "yaml-language-server": "^1.19.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@bijikyu/csup": "^1.0.2",
    "@openai/codex": "^0.53.0",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^2.0.0",
    "@types/opossum": "^8.1.9",
    "@types/sinon": "^21.0.0",
    "agentsqripts": "^1.1.0",
    "arqitect": "^1.0.7",
    "babel-jest": "^30.1.2",
    "commoncontext": "^1.0.3",
    "express-rate-limit": "^7.0.0",
    "fileflows": "^1.0.3",
    "jest": "^29.7.0",
    "loqatevars": "^1.0.6",
    "madge": "^8.0.0",
    "msw": "^2.12.7",
    "multer": "^2.0.2",
    "npmcontext": "^1.0.4",
    "opencode-ai": "1.0.223",
    "qtests": "^2.0.0",
    "quantumagent": "^1.0.5",
    "repomix": "^1.2.0",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3",
    "unqommented": "^1.1.0",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "files": [
    "bin",
    "dist",
    "lib/templates",
    "templates",
    "scripts",
    "qtests-runner.mjs",
    "qtests-runner.mjs.template",
    "README.md",
    "LICENSE"
  ]
}
