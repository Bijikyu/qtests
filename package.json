{
  "name": "qtests",
  "version": "1.1.0",
  "description": "Comprehensive Node.js testing framework with method stubbing, console mocking, environment management, and automatic stub resolution for axios, winston, and other modules",
  "main": "index.js",
  "bin": {
    "qtests-runner": "./bin/qtests-runner",
    "qtests-generate": "./bin/qtests-generate"
  },
  "scripts": {
    "test": "node qtests-runner.js"
  },
  "keywords": [
    "testing",
    "stub",
    "mock",
    "winston",
    "axios",
    "utils",
    "test-framework",
    "console-mock",
    "environment-management",
    "jest-compatible",
    "offline-testing",
    "side-effect-free"
  ],
  "author": "Q",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "winston": "^3.17.0"
  },
  "dependencies": {
    "@types/node": "^24.0.0",
    "axios": "^1.9.0",
    "instruqt": "^1.0.7"
  }
}