entrypoint = "index.js"
modules = ["nodejs-20"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_11"

[deployment]
run = ["node", "index.js"]
deploymentTarget = "autoscale"
ignorePorts = false

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Generate Tests"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Test Suite"

[[workflows.workflow]]
name = "Generate Tests"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx tsx bin/qtests-generate"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Test Suite"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx tsx qtests-runner.ts"

[workflows.workflow.metadata]
outputType = "console"

