entrypoint = "index.js"
modules = ["nodejs-20"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_11"

[deployment]
run = ["node", "index.js"]
deploymentTarget = "autoscale"
ignorePorts = false

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Generate Tests"

[[workflows.workflow]]
name = "Generate Tests"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node bin/qtests-generate"

[[ports]]
localPort = 36203
externalPort = 9000

[[ports]]
localPort = 37193
externalPort = 8081

[[ports]]
localPort = 46843
externalPort = 8099
