{
  "name": "qtests-demo-mern",
  "private": true,
  "version": "0.0.0",
  "description": "Small MERN demo to validate qtests stub injection, routing, and runner integration",
  "type": "commonjs",
  "scripts": {
    "pretest": "node scripts/clean-debug.mjs && node scripts/clean-dist.mjs && node scripts/ensure-runner.mjs && qtests-generate",
    "test": "node qtests-runner.mjs",
    "start:server": "node server/index.js",
    "start:client": "vite --config client/vite.config.ts"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.24.7",
    "babel-jest": "^30.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "qtests": "file:..",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2",
    "vite": "^5.4.0",
    "@vitejs/plugin-react": "^4.3.1"
  }
}