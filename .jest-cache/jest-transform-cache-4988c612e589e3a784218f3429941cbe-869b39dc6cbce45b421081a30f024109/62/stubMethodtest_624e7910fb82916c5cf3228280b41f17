const stubMethod_js_1 = require("../utils/stubMethod.js"); // import stubMethod utility
test('stubMethod replaces and restores methods', () => {
    const obj = { method: () => 'original' };
    const stub = (0, stubMethod_js_1.stubMethod)(obj, 'method', () => 'stubbed');
